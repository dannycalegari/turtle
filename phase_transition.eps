%!PS-Adobe-2.0 EPSF-2.0 
%%BoundingBox: 0 0 440 420 

gsave 
5 5 scale 
1 5 div setlinewidth 
5 40 translate 

/l {4 dict begin /y2 exch def /x2 exch def /y1 exch def /x1 exch def 
newpath x1 y1 moveto x2 y2 lineto stroke end} def 

/draw_graph {5 dict begin
	/V exch def
	0 1 18{
		/i exch def
		/I i 1 add def
		0 1 19{
			/j exch def
			/J j 1 add def
			2 i mul 2 j mul add j i sub V i get j get 8 mul add
			2 I mul 2 j mul add j I sub V I get j get 8 mul add l
			j 19 lt{
				2 i mul 2 j mul add j i sub V i get j get 8 mul add
				2 i mul 2 J mul add J i sub V i get J get 8 mul add l			
			} if
			I 19 eq j 19 lt and {
				2 I mul 2 j mul add j I sub V I get j get 8 mul add
				2 I mul 2 J mul add J I sub V I get J get 8 mul add l				
			} if
		} for
	} for
end} def

/draw_phase_curve { 5 dict begin
	gsave
	1 0 0 setrgbcolor
	newpath
	0 0 moveto
	0 1 57{
		/i exch def
		/ell i 2 div sin def
		/I ell 40 mul def
		/J i 4.5 div def
		2 I mul 2 J mul add J I sub lineto
	} for
	stroke
	grestore
end } def

/draw_axes {
	newpath 0 0 moveto 10 -5 lineto stroke
	newpath 0 0 moveto 10 5 lineto stroke
	newpath 0 0 moveto 0 11.2 lineto stroke
	newpath 3 -5 moveto ( L ) show
	newpath 3 4.5 moveto ( A ) show
	newpath -4 4.5 moveto ( V ) show
} def

