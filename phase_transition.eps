%!PS-Adobe-2.0 EPSF-2.0 
%%BoundingBox: 0 0 480 420 

gsave 
5 5 scale 
1 5 div setlinewidth 
5 40 translate 

/xyz_to_xy {3 dict begin
	/z exch def
	/y exch def
	/x exch def
	2.5 x mul 2 y mul add y x sub z add
end} def

/norm {3 dict begin
	/z exch def
	/y exch def
	/x exch def
	x dup mul y dup mul add z dup mul add sqrt
end} def

/normal {12 dict begin	% component of unit normal in z direction
	/z3 exch def
	/y3 exch def
	/x3 exch def
	/z2 exch def
	/y2 exch def
	/x2 exch def
	/z1 exch def
	/y1 exch def
	/x1 exch def
	/xn y2 y1 sub z3 z1 sub mul z2 z1 sub y3 y1 sub mul sub def
	/yn z2 z1 sub x3 x1 sub mul x2 x1 sub z3 z1 sub mul sub def
	/zn x2 x1 sub y3 y1 sub mul y2 y1 sub x3 x1 sub mul sub def
	zn xn yn zn norm div
end} def

/triangle { 9 dict begin
	/z3 exch def
	/y3 exch def
	/x3 exch def
	/z2 exch def
	/y2 exch def
	/x2 exch def
	/z1 exch def
	/y1 exch def
	/x1 exch def
	gsave
	newpath
	x1 y1 z1 x2 y2 z2 x3 y3 z3 normal setgray
	x1 y1 z1 xyz_to_xy moveto
	x2 y2 z2 xyz_to_xy lineto
	x3 y3 z3 xyz_to_xy lineto
	closepath
	fill
	stroke
	newpath
	0 setlinewidth
	0.7 setgray
	x1 y1 z1 xyz_to_xy moveto
	x2 y2 z2 xyz_to_xy lineto
	x3 y3 z3 xyz_to_xy lineto
	closepath
	stroke
	grestore
end} def

/l {4 dict begin /y2 exch def /x2 exch def /y1 exch def /x1 exch def 
newpath x1 y1 moveto x2 y2 lineto stroke end} def 

/draw_graph {6 dict begin
	/V exch def
	0 1 39{
		/i exch def
		0 1 39{
			/j exch def
			i j V i get j get 2000 mul sqrt
			i 1 add j V i 1 add get j get 2000 mul sqrt
			i j 1 add V i get j 1 add get 2000 mul sqrt
			triangle
			i 1 add j V i 1 add get j get 2000 mul sqrt
			i 1 add j 1 add V i 1 add get j 1 add get 2000 mul sqrt
			i j 1 add V i get j 1 add get 2000 mul sqrt
			triangle
		} for
	} for
end} def


/draw_phase_curve { 5 dict begin
	gsave
	1 0 0 setrgbcolor
	newpath
	0 0 moveto
	0 1 100 {
		/i exch def
		/A i 28.955 mul 100 div def
		/ell A 2 div sin def
		/I A 20 mul 45 div def
		/J ell 80 mul def
		J I 0 xyz_to_xy lineto
	} for
	stroke
	grestore
end } def

/draw_axes {
	newpath 0 0 0 xyz_to_xy moveto 5 0 0 xyz_to_xy lineto stroke
	newpath 0 0 0 xyz_to_xy moveto 0 5 0 xyz_to_xy lineto stroke
	newpath 0 0 0 xyz_to_xy moveto 0 0 13 xyz_to_xy lineto stroke
	newpath 0 0.2 11.5 xyz_to_xy moveto 0 0 13 xyz_to_xy lineto 0 -0.2 11.5 xyz_to_xy lineto stroke
	newpath 0.2 4.25 0 xyz_to_xy moveto 0 5 0 xyz_to_xy lineto -0.2 4.25 0 xyz_to_xy lineto stroke
	newpath 4.25 0.2 0 xyz_to_xy moveto 5 0 0 xyz_to_xy lineto 4.25 -0.2 0 xyz_to_xy lineto stroke

	newpath 3.3 -5.8 moveto ( S ) show
	newpath 2.6 3.8 moveto ( A ) show
	gsave
	/Symbol findfont
	3 scalefont setfont 
	newpath -4 5.3 moveto ( s ) show
	grestore
} def

